<script type="text/template" id="server-uptime-template">
    <div class="table-gauge server-uptime">
        <h2><%=gauge.get('title')%></h2>
        <p class="subtitle"><%=gauge.get('subtitle')%></p>

        <table class="table <% if (gauge.get('striped')){ %>table-striped<% } %>">
            <thead>
                <tr>
                    <th>Server Name</th>
                    <th>Last 30 Days</th>
                    <th>Uptime</th>
                </tr>
            </thead>
            <tbody>
                <%
                    seconds_total = 2592000;

                    sorted_data = {};
                    machine_names = _.pluck(gauge.get('table_data').pluck('data'), 'server_name')

                    _.each(machine_names, function(mname){
                        sorted_data[mname] = [];
                    });

                    gauge.get('table_data').each(function(data_row){
                        d = data_row.get('data');
                        sorted_data[d.server_name].push([new Date(d.date), parseInt(d.downtime)]);
                    });
                %>
                <% _.each(sorted_data, function(days, server_name){ %>
                    <%
                        uptime_percent = 0;
                        downtime_seconds = 0;
                    %>
                    <tr data-id="<%=server_name%>">
                        <td>
                            <%=server_name%>
                        </td>
                        <td>
                            <div class="day-nodes">
                                <% _.each(days, function(day_data){ %>
                                    <% downtime_seconds += day_data[1]; %>
                                    <span class="<% if (day_data[1] > 0){ %>downtime<% } %>" data-date="<%=day_data[0]%>" data-downtime="<%=day_data[1]%>"></span>
                                <% }); %>
                            </div>
                        </td>
                        <td>
                            <% uptime_percent = ((seconds_total - downtime_seconds) / seconds_total) * 100; %>
                            <%=uptime_percent.toString().substr(0, 5)%>%
                        </td>
                    </tr>
                <% }); %>
            </tbody>
        </table>

        <% if (gauge.get('show_gauge_reading_tip') && !_.isEmpty(gauge.get('gauge_reading_tip'))){ %>
            <div class="gauge-reading-tip">
                <p><strong>Gauge Reading Tip</strong></p>
                <div><%=gauge.get('gauge_reading_tip')%></div>
            </div>
        <% } %>
    </div>
</script>
