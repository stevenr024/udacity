            _id: '5304f91546f84a962b62c05b'
            ,service_slugs: ["labtech"]            ,internal: true        });
    
    gauges = new GaugesCollection(gauges_data);

    gauges.each(function(gauge){
         var ids = [], clients = [];

        _.each(gauge.get('service_slugs'), function(slug){
            ids = _.union(ids, service_to_client_ids[slug]);

            _.each(ids, function(id){
                if (id){
                    clients.push(clients_by_id[id]);
                }
            });
        });

        gauge.set('available_clients', clients);
    });

    function doneRefreshingData(){
        var open_gauge_id = refreshingData.gauge.getId();

        $('#roar').alert({
            type: 'green'
            ,title: 'Gauge Data Refreshed!'
            ,content: 'The data for ' + refreshingData.gauge.get('title') + ' has been refreshed. Click this message to reopen it.'
            ,duration: 12000
            ,autoShow: true
            ,onClick: function(){
                $('#gauge-' + open_gauge_id).find('.edit-in-builder').click();
            }
        });

        $('.chart-overlay, .secondary-message').fadeOut(700);

        //reset refreshing data
        refreshingData.active = false;
        refreshingData.gauge = null;
    }

    $(function(){
       
        modal_view = new ModalView({
            el: '#details-modal-content'
            ,collection: gauges
        });

        $('.alert .close').on('click', function(e){
            e.preventDefault();

            $(this).closest('.alert').fadeOut(700);
        });

        //need to handle this click otherwise it will fall into the next click event due to
        //the icon bubbling up to the li.gauge element
        $('.icon.edit').click(function(e){
            if (!$(this).hasClass('disabled')){
                window.location = $(this).attr('href');
            }

            e.preventDefault();
            e.stopPropagation();
        });

        $('.gauge-title, .edit-in-builder, li.gauge').click(function(e){
            e.preventDefault();

            var id = $(this).closest('li.gauge').data('id')
                ,gauge_type = $(this).closest('li.gauge').data('gauge-type')
            ;

            modal_view.setGauge(id, function(gauge){
                this.render(gauge);
                $('#details-modal').modal('show')
            });

            //no bubbling
            return false;
        });

        $('.gauge').each(function(){
            if ($(this).data('gauge-type') == 'server_uptime'){
                $(this).find('.edit').tooltip(cannot_edit_tooltip).addClass('disabled');
            }
        });

        $('.actions .refresh-16').on('click', function(e){
            var $overlay = $(this).closest('.details').find('.chart-overlay')
                ,messageTimeout = null
                ,gauge_id = $(this).data('id')
            ;

            refreshingData.active = true;
            refreshingData.gauge = gauges.get(gauge_id);

            e.preventDefault();

            $overlay.fadeIn(700);
            $('.chart-overlay p:first').css('marginTop', $('.chart-overlay').height() / 2 - 70)

            messageTimeout = setTimeout(function(){
                $overlay.find('.secondary-message').fadeIn(1400);
            }, 20000);

            //create job
            create_job(job_types.refresh_gauge, { gauge_id: gauge_id },
                //begin monitoring job
                function(msg){
                    if (msg.results.length){
                        monitor_job(msg.results[0].id,
                        {
                            onError: function(err){
                                if (messageTimeout){
                                    clearTimeout(messageTimeout);
                                }
                            },
                            onComplete: function(){
                                if (messageTimeout){
                                    clearTimeout(messageTimeout);
                                }

                                doneRefreshingData();
                            }
                        });
                    }
                }
            );

        });

                
        //these are only the delete icons that are available on page load, which is the main list.
        //the modal is handled by the backbone ModalView
        $('.delete').click(function(e){
            var id = $(e.currentTarget).data('id');

            e.preventDefault();
            e.stopPropagation();

            if (confirm('Are you sure you want to delete this gauge?')){
                $('#gauge-' + id).fadeOut(350);
                $('#generic-modal').modal('hide');
                $.jsonrpc(
                    {
                        url : '/rpcapi/'
                        ,method : 'deactivate_gauge'
                        ,params : {
                            gauge_id: id
                        }
                    },
                    {
                        success : function(msg) {
                            $('#gauge-' + id).remove();
                        },
                        fault : function(msg) {
                            alert('Error deleting gauge, please try again');
                            $('#generic-modal').modal('hide');
                        }
                    }
                );
            }

            //APP-1812
            //make sure this doesn't bubble up otherwise the gauge modal attempts to open
            //after deletion and the fetch gauge data call fails since the gauge has been deleted
            return false;
        });

        $('.clone').click(function(e){
            e.preventDefault();
            e.stopPropagation();

            var data = {
                clone_id: $(this).data('id')
                ,clone_name: $(this).data('name')
            };

            openModal({
                title: 'Clone Gauge'
                ,content: _.template($('#clone-gauge-modal-template').html(), data)
                ,submit: {
                    text: 'Clone'
                    ,fn: function(e){
                        e.preventDefault();
                        $('#clone-form').submit();
                    }
                }
            });
        });
    });
</script>

            </div>

                            <div class="row-fluid">
                    <div class="span12">
                        <div id="footer">
</div>

                    </div>
                </div>
                    </div>

        
