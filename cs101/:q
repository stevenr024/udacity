<script type="text/template" id="no-data-template">
    <div class="table-gauge no-data">
        <h2><%=gauge.get('title')%></h2>
        <p class="subtitle"><%=gauge.get('subtitle')%></p>

        <div class="msg">
            <p>This data is currently unavailable</p>
        </div>
    </div>
</script>
<script type="text/template" id="table-preview-template">
    <div class="table-gauge" data-attr="aaAAAA">
        <% if (!_.get(options, 'hide_titles', false)){ %>
            <h2><%=gauge.get('title')%></h2>
            <p class="subtitle"><%=gauge.get('subtitle')%></p>
        <% } %>

        <div class="table-container">
            <table class="table table-bordered table-hover <% if (gauge.get('striped')){ %>table-striped<% } %>">
                <% if (gauge.dataHasPivot()){ /* pivoted data output */ %>
                    <% pivot = gauge.get('pivot'); %>
                    <thead>
                        <tr>
                            <% if (!_.isEmpty(pivot.primary_dim)){ %>
                                <th><%=pivot.primary_dim%></th>
                            <% } else { %>
                                <th>&nbsp;</th>
                            <% } %>
                            <th><%=pivot.measure_by + ' of ' + pivot.measure_field%></th>
                        </tr>
                    </thead>
                    <tfoot><tr><td></td></tr></tfoot>
                    <tbody>
                        <%
                            function get_display_val(val){
                                var display = '';

                                //if it's an object, it's probably a date so must manipulate first
                                if (_.isObject(val)){
                                    if (_.has(val, '$date')){
                                        display = moment(get_utc_time(val.$date)).calendar();
                                    }
                                    else if (_.has(val, 'sec')){
                                        display = moment(get_utc_time(val.sec * 1000)).calendar();
                                    }
                                    //
                                    else{
                                        console.log('drilldown cant display', display);
                                    }
                                }
                                //if it's a string it's ready to go
                                else if (_.isString(val)){
                                    display = val;
                                }
                                else if (val == null){
                                    val = "null";
                                }
                                //everything else use toString
                                else{
                                    display = val.toString();
                                }

                                display = display.replace(new RegExp('\r?\n', 'g'), '<br />')

                                return display;
                            }
                        %>
                        <% gauge.get('table_data').each(function(row, key){ %>
                            <tr data-id="<%=key%>">
                                <td>
                                    <%=get_display_val(row.get('data')._id)%>
                                </td>
                                <td>
                                    <%=get_display_val(row.get('data')[pivot.measure_field])%>
                                </td>
                            </tr>
                        <% }); %>
                <% } else { /* regular raw data output */ %>
                    <thead>
                        <tr>
                            <% gauge.get('table_columns').each(function(col){ %>
                                <% if (col.get('active')){ %>
                                    <th data-id="<%= col.get('name') %>">
                                        <%=col.get('display')%>
                                    </th>
                                <% } %>
                            <% }); %>
                        </tr>
                    </thead>
                    <tfoot><tr><td></td></tr></tfoot>
<tbody>
<% gauge.get('table_data').each(function(row, key){ %>
<tr data-id="<%=key%>">
<% gauge.get('table_columns').each(function(col){ %>
<% if (col.get('active')){ %>
<td>
<% if (col.get('conditional')){ %>
<% passed = false; %>
<% col.get('conditions').each(function(condition){ %>
<% if (!passed && condition.evaluate(row.getColumnValue(col.get('name')))){ %>
<% passed = true; %>
<%=condition.getDisplay(row.getColumnValue(col.get('name')))%>
<% } %>
<% }); %>
<% } else { %>
<%=row.getColumnValue(col.get('name'))%>
<% } %>
</td>
<% } %>
<% }); %>
</tr>
<% }); %>
</tbody>
                <% } /*end else for no pivot */ %>
            </table>
        </div>
        <%
            show_tip = gauge.get('show_gauge_reading_tip') && !_.isEmpty(gauge.get('gauge_reading_tip'));

            if (options && _.has(options, 'show_gauge_reading_tip')){
                show_tip = options.show_gauge_reading_tip;
            }
        %>

        <% if (gauge.get('gauge_reading_tip')){ %>
            <div class="gauge-reading-tip">
                <p><strong>Gauge Reading Tip</strong></p>
                <div><%=gauge.get('gauge_reading_tip')%></div>
            </div>
        <% } %>
    </div>
</script>

<script type="text/template" id="table-drilldown-template">
    <table class="table table-striped table-drilldown">
        <thead>
            <tr class="title-row drilldown-title">
                <th>&nbsp;</th>
                <th style="text-align:right;">
                    <a href="#" class="back-to-chart"></a>
                </th>
            </tr>
        </thead>
        <tbody>
            <% columns.each(function(col, colNum){ %>
                <% if (_.has(data, col.get('name'))){ %>
                    <%
                        var val = data[col.get('name')];
                        var display = '';

                        //if it's an object, it's probably a date so must manipulate first
                        if (_.isObject(val)){
                            if (_.has(val, '$date')){
                                display = moment(get_utc_time(val.$date)).calendar();
                            }
                            else if (_.has(val, 'sec')){
                                display = moment(get_utc_time(val.sec * 1000)).calendar();
                            }
                            //
                            else{
                                console.log('drilldown cant display', display);
                            }
                        }
                        //if it's a string it's ready to go
                        else if (_.isString(val)){
                            display = val;
                        }
                        else if (val == null){
                            display = "null";
                        }
                        //everything else use toString
                        else{
                            display = val.toString();
                        }

                        display = display.replace(new RegExp('\r?\n', 'g'), '<br />')
                    %>
                    
                <% } %>
                <% if (col.get('active')){ %>
                    <tr data-col-num=<%= colNum %>>
                        <td >
                            <span data-id="<%=col.get('name')%>" class="column-label"><strong><%=col.get('display')%></strong></span>
                        </td>
                        <td>
                            <%=display%>
                        <td>
                    </tr>
                <% } else if ($('#view-report').length == 0){ %>
                    <tr data-col-num=<%= colNum %> style="display: none;" >
                        <td >
                            <span data-id="<%=col.get('name')%>" class="column-label"><strong><%=col.get('display')%></strong></span>
                        </td>
                        <td>
                            <%=display%>
                        <td>
                    </tr>
                <% } %>
            <% }); %>
        </tbody>
    </table>
</script>
