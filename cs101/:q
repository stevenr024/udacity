<script type="text/javascript">
    /** TODO: Stop being lazy and move this into an appropriate place! **/
    var ModalView = Backbone.View.extend({
        template: null
        ,model: null
        ,collection: null
        ,initialize: function(opts){
            opts = _.extend({
                collection: new GaugesCollection()
            }, opts || {});

            this.collection = opts.collection;
            this.template = _.template($('#details-modal-template').html());
        }
        ,events: {
            'click .close-dialog': function(e){
                e.preventDefault();

                $('#details-modal').modal('hide');
            }
            ,'click .edit.disabled': function(e){
                return false;
            }
            ,'click .tabs a': function(e){
                var $el = $( $(e.currentTarget).attr('href'));

                e.preventDefault();

                if ($el.length){
                    $(e.currentTarget).closest('ul').find('.active').removeClass('active');
                    $(e.currentTarget).parent('li').addClass('active');

                    $el.closest('.tab-content').find('.active').removeClass('active');
                    $el.addClass('active');
                }
            }
            ,'click .delete': function(e){
                //note***
                //this method is only for the delete icon in the modal, the ones that are in the
                //main list are handled with a normal click event
                
                var id = $(e.currentTarget).data('id');

                e.preventDefault();

                if (confirm('Are you sure you want to delete this gauge?')){
                    $('#generic-modal').modal('show');
                    $.jsonrpc(
                        {
                            url : '/rpcapi/'
                            ,method : 'deactivate_gauge'
                            ,params : {
                                gauge_id: id
                            }
                        },
                        {
                            success : function(msg) {
                                $('#gauge-' + id).fadeOut(350, function(){ $(this).remove(); });
                                $('#generic-modal').modal('hide');
                            },
                            fault : function(msg) {
                                alert('Error deleting gauge, please try again');
                                $('#generic-modal').modal('hide');
                            }
                        }
                    );
                }
            }
            ,'click .customers .select-all': function(e){
                var $checks = $(e.currentTarget).closest('.customers').find('input:checkbox');

                if ($(e.currentTarget).is(':checked')){
                    $checks.attr('checked', 'checked');
                }
                else{
                    $checks.removeAttr('checked');
                }

                $(e.currentTarget).closest('.customers').find('.apply').addClass('green').removeClass('disabled');
            }
            ,'click .customer-list input:checkbox': function(e){
                $(e.currentTarget).closest('.customers').find('.apply').addClass('green').removeClass('disabled');
            }
            ,'click .apply': function(e){
                var gauge_id = $(e.currentTarget).attr('href').replace('#', '')
                    ,customer_ids = []
                    ,$apply = $(e.currentTarget)
                ;

                e.preventDefault();

                $(e.currentTarget).closest('.gauge').find('.customer-list input:checkbox:checked').each(function(){
                    customer_ids.push($(this).val());
                });

                $.jsonrpc(
                    {
                        url : '/rpcapi/'
                        ,method : 'set_gauge_clients'
                        ,params : {
                            gauge_id: gauge_id
                            ,client_ids: customer_ids
                        }
                    },
                    {
                        success : function(msg) {
                            var $select = $('#details-modal .view-as-client')
                                ,$addClientsLink = $('#details-modal .assign-to-clients')
                                ,$checks = $('#details-modal .customer-list input:checkbox:checked')
                                ,html = ''
                            ;

                            //if the gauge wasn't assigned to any clients before, then we need to hide the
                            //"assign to clients" link and create a select in it's place
                            if (!$select.length){
                                $select = $('<select class="view-as-client" data-id="' + gauge_id + '"></select>');
                                $addClientsLink.replaceWith($select);
                            }

                            //there are now clients assigned so let's update the "view as" <select> with all the
                            //currently selected clients to make sure any new ones are instantly available
                            if ($checks.length){
                                $select.html('');

                                //loop through each check and add the html for another option
                                $checks.each(function(){
                                    html += _.template(
                                        '<option value="<%=val%>"><%=label%></option>'
                                        ,{
                                            val: $(this).val()
                                            ,label: $(this).parent().text()
                                        }
                                    );
                                });

                                //insert all the new options
                                $select.html(html).change();
                            }
                            //if there are no checks, we need to make sure the "assign to clients" link shows up
                            else{
                                //create the link
                                $addClientsLink = $('<a href="#" class="assign-to-clients">Assign to Clients</a>');
                                //replace the select with it
                                $select.replaceWith($addClientsLink);
                                //replace the chart with the generic text
                                $('#gauge-' + gauge_id + ' .chart-container').html('<p style="padding:0 10px 10px;">This gauge is not currently assigned to any clients.</p>');
                            }

                            $apply.removeClass('green').addClass('disabled');
                        },
                        fault : function(msg) {
                            alert('Error attaching gauge to clients, please try again');
                        }
                    }
                );
            }
            ,'change .view-as-client': function(e){
                var client_id = $(e.currentTarget).val()
                    ,gauge_id = $(e.currentTarget).data('id')
                ;

                this.getChart(gauge_id, client_id);
            }
            ,'click .assign-to-clients': function(e){
                e.preventDefault();

                $(e.currentTarget).closest('.gauge').find('.tabs .clients').click();
            }
        }
        ,getChart: function(gauge_id, client_ids){
            var chart_container = '#chart-container-' + gauge_id;

            if (_.isArray(client_ids)){
                _.each(client_ids, function(v, k){
                    if (!_.isNaN(parseInt(v))){
                        client_ids[k] = parseInt(v);
                    }
                });
            }
            else{
                client_ids = [parseInt(client_ids)];
            }

            gauges.getChart({
                gauge_id: gauge_id
                ,chart_el: chart_container
                ,client_id: client_ids
                ,chart_height: 380
                ,chart_overrides: {
                    hide_titles: true
                    ,hide_exporting: true
                }
            }, function(){
                //this gives the chart a chance to render then the modal will open having factored in its height
                setTimeout(function(){
                    $('#details-modal').modal('show');
                }, 100);
            });
        }
        ,setGauge: function(id, fn){
            this.model = this.collection.get(id);

            var available_clients = this.model.get('available_clients')
                ,first_client_id = -1
                ,self = this
                ,gauge = gauges.get(id)
                ,chart_opts = {
                    gauge_id: id
                    ,chart_el: '#chart-container-' + id
                    ,return_data: true
                    ,chart_height: 380
                    ,chart_overrides: {
                        hide_titles: true
                        ,hide_exporting: true
                    }
                }
            ;

            if (available_clients.length){
                first_client_id = available_clients[0].id;
            }

            if (gauge){
                if (gauge.get('internal')){
                    //set to empty
                    chart_opts.client_ids = [];

                    //apply all valid ids cast to int
                    _.each(_.pluck(available_clients, 'id'), function(id){
                        if (!_.isNaN(parseInt(id))){
                            chart_opts.client_ids.push(parseInt(id));
                        }
                    });

                    chart_opts.limit = 1000;
                }
                else{
                    chart_opts.client_id = parseInt(first_client_id);
                }
            }

            gauges.getChart(chart_opts, function(gauge_model){
                var clients_with_data = [];

                //reapply the available clients
                gauge_model.set('available_clients', available_clients);

                _.each(gauge_model.get('clients'), function(c_id){
                    if (_.has(clients_by_id, c_id)){
                        clients_with_data.push(clients_by_id[c_id]);
                    }
                });

                gauge_model.set('clients_with_data', clients_with_data);

                self.model = gauge_model;

                if (fn){
                    fn.call(self, gauge_model);
                }
            });

            // this.model.fetch({
            //     success: function(model){
            //         var clients_with_data = [];

            //         _.each(model.get('clients'), function(c_id){
            //             if (_.has(clients_by_id, c_id)){
            //                 clients_with_data.push(clients_by_id[c_id]);
            //             }
            //         });

            //         model.set('clients_with_data', clients_with_data);

            //         if (fn){
            //             fn(model);
            //         }
            //     }
            // });
        }
        ,render: function(gauge){
            if (gauge){
                this.model = gauge;
            }

            this.$el.empty();

            this.$el.html(this.template({
                gauge: this.model
            }));

            if (_.isEmpty(this.model.get('available_clients'))){
                $('#details-modal').modal('show');
            }
            else{
                if (this.model.get('internal')){
                    this.getChart(this.model.getId(), _.pluck(this.model.get('available_clients'), 'id'));
                }
                else{
                    this.$el.find('.view-as-client').change();
                }
            }
        }
    });

    var service_to_client_ids = {"connectwise":["87138"],"labtech":["87138"]};
    var clients_by_id = {"87138":{"id":"87138","name":"All Clients","service_slugs":["connectwise","labtech"]}};

    var $detailsModal = null
        ,gauges = null
        ,gauges_data = []
        ,modal_view = null
        ,refreshingData = {
            active: false
            ,gauge: null
        }
        ,cannot_edit_tooltip = { position:'top', 'title':'This gauge cannot be edited.' }
    ;

            
        gauges_data.push({
            _id: '5367f6c146f84a1b633decdf'
            ,service_slugs: ["connectwise"]            ,internal: true        });
            
        gauges_data.push({
